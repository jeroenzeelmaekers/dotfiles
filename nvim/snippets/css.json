{
  "Flexbox Container": {
    "prefix": "flex",
    "body": [
      "display: flex;",
      "justify-content: ${1|center,flex-start,flex-end,space-between,space-around,space-evenly|};",
      "align-items: ${2|center,flex-start,flex-end,stretch,baseline|};$0"
    ],
    "description": "Flexbox container"
  },
  "Flexbox Column": {
    "prefix": "flex-col",
    "body": [
      "display: flex;",
      "flex-direction: column;",
      "justify-content: ${1|center,flex-start,flex-end,space-between,space-around|};",
      "align-items: ${2|center,flex-start,flex-end,stretch|};$0"
    ],
    "description": "Flexbox column container"
  },
  "Flexbox Center": {
    "prefix": "flex-center",
